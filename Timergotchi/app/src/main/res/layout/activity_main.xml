<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryLight"
    android:paddingTop="0dp">

    <TextView
        android:id="@+id/pointsview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:gravity="center"
        android:text="Points"
        android:textColor="@color/colorPrimary"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/listtabs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47000003" />

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/listpager"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listtabs">

    </androidx.viewpager.widget.ViewPager>

    <FrameLayout
        android:id="@+id/nav"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@color/design_default_color_on_primary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:id="@+id/navbar"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="1dp"
            android:background="@color/colorPrimaryLight"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/tolist"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:tint="@color/colorPrimary"
                app:srcCompat="@drawable/lists" />

            <ImageView
                android:id="@+id/totimer"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="toTimer"
                android:tint="@color/colorPrimaryMedium"
                app:srcCompat="@drawable/timer" />

            <ImageView
                android:id="@+id/topet"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="toPet"
                android:tint="@color/colorPrimaryMedium"
                app:srcCompat="@drawable/pet" />

            <ImageView
                android:id="@+id/toshop"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="toShop"
                android:tint="@color/colorPrimaryMedium"
                app:srcCompat="@drawable/rewards" />

            <ImageView
                android:id="@+id/torecord"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="toRecord"
                android:tint="@color/colorPrimaryMedium"
                app:srcCompat="@drawable/record" />
        </LinearLayout>
    </FrameLayout>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/listtabs"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:background="@color/colorPrimaryLight"
        android:foregroundGravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tabGravity="fill"
        app:tabIconTint="@color/colorPrimary"
        app:tabIndicatorColor="@color/colorPrimary"
        app:tabIndicatorFullWidth="false"
        app:tabIndicatorGravity="center"
        app:tabIndicatorHeight="0dp"
        app:tabSelectedTextColor="@color/colorPrimary"
        app:tabTextColor="@color/colorPrimaryLight">

        <com.google.android.material.tabs.TabItem
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="left"
            android:foregroundGravity="left"
            android:text="Monday" />

    </com.google.android.material.tabs.TabLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/edit"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/colorPrimaryLight"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/editmenu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorPrimaryLight"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <EditText
                android:id="@+id/editname"

                android:layout_width="0dp"
                android:layout_height="44dp"
                android:layout_marginStart="44dp"
                android:layout_marginTop="68dp"
                android:layout_marginEnd="44dp"
                android:background="@drawable/task"
                android:ems="10"
                android:focusable="auto"
                android:gravity="left|center_vertical"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:selectAllOnFocus="false"
                android:shadowColor="@color/colorAccent"
                android:textColor="@color/textColor"
                android:textColorHighlight="@color/colorAccent"
                android:textColorHint="@color/colorAccent"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/repeatbar"

                android:layout_width="0dp"
                android:layout_height="47dp"
                android:layout_marginStart="44dp"
                android:layout_marginEnd="44dp"
                android:background="@drawable/task"
                android:focusable="auto"
                android:paddingTop="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dueDate" />

            <ImageView
                android:id="@+id/editnameleft"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_left"
                app:layout_constraintBottom_toBottomOf="@+id/donebutton"
                app:layout_constraintEnd_toStartOf="@+id/editname"
                app:layout_constraintStart_toStartOf="@+id/donebutton"
                app:layout_constraintTop_toTopOf="@+id/editname" />

            <ImageView
                android:id="@+id/duedateleft"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_left"
                app:layout_constraintBottom_toBottomOf="@+id/dueDate"
                app:layout_constraintEnd_toStartOf="@+id/dueDate"
                app:layout_constraintStart_toStartOf="@+id/donebutton"
                app:layout_constraintTop_toTopOf="@+id/dueDate" />

            <ImageView
                android:id="@+id/repeatleft"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_left"
                app:layout_constraintEnd_toStartOf="@+id/editname"
                app:layout_constraintStart_toStartOf="@+id/donebutton"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <ImageView
                android:id="@+id/repeatright"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_right"
                app:layout_constraintStart_toEndOf="@+id/editname"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <ImageView
                android:id="@+id/editnameright"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_right"
                app:layout_constraintBottom_toBottomOf="@+id/editname"
                app:layout_constraintStart_toEndOf="@+id/editname"
                app:layout_constraintTop_toTopOf="@+id/editname" />

            <ImageView
                android:id="@+id/editdelete"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:onClick="onDeletePopup"
                android:tint="@color/colorAccent"
                app:layout_constraintStart_toEndOf="@+id/editname"
                app:layout_constraintTop_toTopOf="@+id/editname"
                app:srcCompat="@drawable/delete" />

            <TextView
                android:id="@+id/dueDate"

                android:layout_width="0dp"
                android:layout_height="44dp"
                android:layout_marginStart="44dp"

                android:layout_marginTop="20dp"
                android:layout_marginEnd="44dp"
                android:background="@drawable/task"
                android:focusable="auto"
                android:gravity="center"
                android:paddingRight="20dp"
                android:textColor="@color/textColor"
                android:textColorHighlight="@color/colorAccent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editname" />

            <ImageView
                android:id="@+id/clearduedate"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_right"
                android:onClick="clearDueDate"
                android:src="@drawable/cross"
                android:tint="@color/colorAccent"
                app:layout_constraintBottom_toBottomOf="@+id/dueDate"
                app:layout_constraintStart_toEndOf="@+id/dueDate"
                app:layout_constraintTop_toTopOf="@+id/dueDate" />

            <TextView
                android:id="@+id/duedatetitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:onClick="onCalendarPopup"
                android:text="Due Date"
                android:textColor="@color/textColor"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="@+id/dueDate"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.002"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/dueDate" />

            <TextView
                android:id="@+id/repeattitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:text="Repeat"
                android:textColor="@color/textColor"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editthu2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="T"
                android:textColor="@color/design_default_color_surface"
                app:drawableTint="@color/colorAccent"
                app:drawableTintMode="add"
                app:layout_constraintBottom_toBottomOf="@+id/editthu"
                app:layout_constraintEnd_toEndOf="@+id/editthu"
                app:layout_constraintHorizontal_bias="0.67"
                app:layout_constraintStart_toStartOf="@+id/editthu"
                app:layout_constraintTop_toTopOf="@+id/editthu" />

            <TextView
                android:id="@+id/edittue2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="T"
                android:textColor="@color/design_default_color_surface"
                app:drawableTint="@color/colorAccent"
                app:drawableTintMode="add"
                app:layout_constraintBottom_toBottomOf="@+id/edittue"
                app:layout_constraintEnd_toEndOf="@+id/edittue"
                app:layout_constraintHorizontal_bias="0.33"
                app:layout_constraintStart_toStartOf="@+id/edittue"
                app:layout_constraintTop_toTopOf="@+id/edittue" />

            <TextView
                android:id="@+id/editfri2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="F"
                android:textColor="@color/design_default_color_surface"
                app:drawableTint="@color/colorAccent"
                app:drawableTintMode="add"
                app:layout_constraintBottom_toBottomOf="@+id/editfri"
                app:layout_constraintEnd_toEndOf="@+id/editfri"
                app:layout_constraintHorizontal_bias="0.75"
                app:layout_constraintStart_toStartOf="@+id/editfri"
                app:layout_constraintTop_toTopOf="@+id/editfri" />

            <TextView
                android:id="@+id/editsat2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="S"
                android:textColor="@color/design_default_color_background"
                app:drawableTint="@color/colorAccent"
                app:drawableTintMode="add"
                app:layout_constraintBottom_toBottomOf="@+id/editsat"
                app:layout_constraintEnd_toEndOf="@+id/editsat"
                app:layout_constraintHorizontal_bias="0.9"
                app:layout_constraintStart_toStartOf="@+id/editsat"
                app:layout_constraintTop_toTopOf="@+id/editsat" />

            <TextView
                android:id="@+id/editmon2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="M"
                android:textColor="@color/design_default_color_on_primary"
                app:drawableTint="@color/colorAccent"
                app:drawableTintMode="add"
                app:layout_constraintBottom_toBottomOf="@+id/editmon"
                app:layout_constraintEnd_toEndOf="@+id/editmon"
                app:layout_constraintHorizontal_bias="0.17"
                app:layout_constraintStart_toStartOf="@+id/editmon"
                app:layout_constraintTop_toTopOf="@+id/editmon" />

            <TextView
                android:id="@+id/editsun2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="S"
                android:textColor="@color/design_default_color_background"
                app:drawableTint="@color/colorAccent"
                app:layout_constraintBottom_toBottomOf="@+id/editsun"
                app:layout_constraintEnd_toEndOf="@+id/editsun"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/editsun"
                app:layout_constraintTop_toTopOf="@+id/editsun" />

            <TextView
                android:id="@+id/editwed2"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/buttonbackon"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="W"
                android:textColor="@color/design_default_color_on_primary"
                app:drawableTint="@color/colorAccent"
                app:drawableTintMode="add"
                app:layout_constraintBottom_toBottomOf="@+id/editwed"
                app:layout_constraintEnd_toEndOf="@+id/editwed"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/editwed"
                app:layout_constraintTop_toTopOf="@+id/editwed" />

            <TextView
                android:id="@+id/edittue"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="T"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.35"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editwed"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="W"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editthu"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="T"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.65"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editfri"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="F"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editsat"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="S"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.95"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editmon"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="M"
                android:textColor="@color/textColor"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/repeatbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/repeatbar" />

            <TextView
                android:id="@+id/editsun"
                android:layout_width="44dp"
                android:layout_height="35dp"
                android:layout_marginStart="60dp"
                android:background="@color/design_default_color_background"
                android:gravity="center"
                android:onClick="onRepeat"
                android:text="S"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/editwed"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.05"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/editwed" />

            <TextView
                android:id="@+id/editcancel"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:background="@drawable/dateback"
                android:gravity="center"
                android:onClick="onEditCancel"
                android:text="CANCEL"
                android:textColor="@color/textColor"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.25"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/editok" />

            <TextView
                android:id="@+id/editok"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:layout_marginBottom="40dp"
                android:background="@drawable/dateback"
                android:gravity="center"
                android:onClick="onEditOK"
                android:text="@android:string/ok"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.75"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/showDo"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/done"
                app:layout_constraintBottom_toBottomOf="@+id/donebutton"
                app:layout_constraintEnd_toStartOf="@+id/editname"
                app:layout_constraintStart_toStartOf="@+id/donebutton"
                app:layout_constraintTop_toTopOf="@+id/editname" />

            <ImageView
                android:id="@+id/label1right"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_right"
                android:onClick="setColor"
                app:layout_constraintStart_toEndOf="@+id/label1"
                app:layout_constraintTop_toTopOf="@+id/label1" />

            <ImageView
                android:id="@+id/label1left"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/task_bar_end_left"
                app:layout_constraintBottom_toBottomOf="@+id/donebutton"
                app:layout_constraintEnd_toStartOf="@+id/label1"
                app:layout_constraintStart_toStartOf="@+id/donebutton"
                app:layout_constraintTop_toTopOf="@+id/label1" />

            <EditText
                android:id="@+id/label1"

                android:layout_width="0dp"
                android:layout_height="44dp"
                android:layout_marginStart="44dp"
                android:layout_marginEnd="44dp"
                android:background="@drawable/task"
                android:ems="10"
                android:focusable="auto"
                android:gravity="left|center_vertical"
                android:imeOptions="actionDone"
                android:inputType="text"
                android:selectAllOnFocus="false"
                android:shadowColor="@color/colorAccent"
                android:textColor="@color/textColor"
                android:textColorHighlight="@color/colorAccent"
                android:textColorHint="@color/colorAccent"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/repeatbar" />

            <ImageView
                android:id="@+id/colorcode1"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:background="@drawable/done"
                android:backgroundTint="@color/colorPastelRed"
                app:layout_constraintEnd_toStartOf="@+id/label1"
                app:layout_constraintTop_toTopOf="@+id/label1left" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/calendarPopup"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorShadow"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/editdeleteok2"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/dateback"
                android:gravity="center"
                android:onClick="onDelete"
                android:text="@android:string/ok"
                android:textColor="@color/textColor"
                app:layout_constraintEnd_toEndOf="@+id/calendarView"
                app:layout_constraintHorizontal_bias="0.75"
                app:layout_constraintStart_toStartOf="@+id/calendarView"
                app:layout_constraintTop_toBottomOf="@+id/calendarView" />

            <TextView
                android:id="@+id/deletecancel2"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:background="@drawable/dateback"
                android:gravity="center"
                android:onClick="onDeleteCancel"
                android:text="CANCEL"
                android:textColor="@color/textColor"
                app:layout_constraintEnd_toEndOf="@+id/calendarView"
                app:layout_constraintHorizontal_bias="0.25"
                app:layout_constraintStart_toStartOf="@+id/calendarView"
                app:layout_constraintTop_toTopOf="@+id/editdeleteok2" />

            <CalendarView
                android:id="@+id/calendarView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/calendarback"
                android:unfocusedMonthDateColor="@color/textColor"
                android:weekNumberColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/deletepopup"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/colorShadow"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="230dp"
                android:layout_height="80dp"
                android:layout_marginStart="44dp"
                android:layout_marginLeft="44dp"
                android:layout_marginEnd="44dp"
                android:layout_marginRight="44dp"
                android:background="@drawable/calendarback"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="Are you sure you want to delete?"
                android:textColor="@color/textColor"
                app:layout_constraintEnd_toEndOf="@+id/imageView11"
                app:layout_constraintHorizontal_bias="0.534"
                app:layout_constraintStart_toStartOf="@+id/imageView11"
                app:layout_constraintTop_toTopOf="@+id/imageView11" />

            <TextView
                android:id="@+id/editdeleteok"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:onClick="onDelete"
                android:text="@android:string/ok"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/imageView11"
                app:layout_constraintEnd_toEndOf="@+id/imageView11"
                app:layout_constraintHorizontal_bias="0.9"
                app:layout_constraintStart_toStartOf="@+id/imageView11" />

            <TextView
                android:id="@+id/deletecancel"
                android:layout_width="100dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:onClick="onDeleteCancel"
                android:text="CANCEL"
                android:textColor="@color/textColor"
                app:layout_constraintBottom_toBottomOf="@+id/imageView11"
                app:layout_constraintEnd_toEndOf="@+id/imageView11"
                app:layout_constraintHorizontal_bias="0.1"
                app:layout_constraintStart_toStartOf="@+id/imageView11" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>